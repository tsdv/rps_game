$("#modal-wins").find(".modal-content").html("<%= j(render 'games/modal_wins') %>");
$('#curb-choose').attr('src', "<%=j(image_path("#{@result[:curb_bet]}.svg"))%>")
$('#modal-waiting').on('hidden.bs.modal', function () {
